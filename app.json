{
	"expo": {
		"name": "Earny",
		"slug": "Earny",
		"version": "1.0.1",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "automatic",
		"scheme": "io.earny.earny",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "cover",
			"backgroundColor": "#ffffff"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": false,
			"bundleIdentifier": "io.Earny.Earny"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"package": "io.earny.earny"
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"plugins": [
			[
				"expo-router",
				{
					"origin": "./assets"
				}
			],
			[
				"expo-build-properties",
				{
					"ios": {
						"useFrameworks": "static"
					}
				}
			]
		],
		"extra": {
			"router": {
				"origin": "./assets"
			},
			"eas": {
				"projectId": "4dfd197c-ef0e-4e99-840b-843cbc6018d2"
			}
		}
	}
}

{
  "expo": {
    "name": "Earny",
    "slug": "Earny",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "io.earny.earny",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "io.earny.earny"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "./assets"
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": "./assets"
      },
      "eas": {
        "projectId": "4dfd197c-ef0e-4e99-840b-843cbc6018d2"
      }
    }
  }
}

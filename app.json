{
	"expo": {
		"name": "Earny",
		"slug": "earny",
		"version": "1.0.3",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "light",
		"scheme": "earny",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "cover",
			"backgroundColor": "#ffffff"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": false,
			"bundleIdentifier": "io.Earny.Earny"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			}
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"plugins": [
			[
				"expo-build-properties",
				{
					"ios": {
						"useFrameworks": "static"
					}
				}
			],
			[
				"expo-camera",
				{
					"cameraPermission": "Allow Earny to access your camera.",
					"microphonePermission": "Allow Earny to access your microphone."
				}
			]
		],
		"extra": {
			"eas": {
				"projectId": "2d41210b-be24-4ae0-9662-63009061cdac"
			}
		}
	}
}
